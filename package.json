{
  "name": "clerk-multitenant-cf-workers-example",
  "private": true,
  "description": "All-in-one remix starter template for Cloudflare Pages",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "cleanup": "rimraf .cache ./build",
    "deploy": "NODE_ENV=production wrangler deploy",
    "dev": "remix vite:dev",
    "format": "prettier --write .",
    "lint": "biome lint .",
    "start": "wrangler dev",
    "typecheck": "wrangler types && tsc",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "@clerk/remix": "^4.2.41",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@remix-run/cloudflare": "^2.13.1",
    "@remix-run/cloudflare-pages": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "@remix-run/server-runtime": "^2.13.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "isbot": "^5.1.17",
    "lucide-react": "^0.454.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/workers-types": "^4.20241022.0",
    "@octokit/types": "^13.6.1",
    "@remix-run/dev": "^2.13.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^5.0.1",
    "wrangler": "^3.83.0"
  },
  "engines": {
    "node": ">=20"
  }
}
